{
	"manifest_version": 2,

	"name": "Instagram Picture Grabber",
	"description": "Grab instagram photos and download them to computer.",
	"version": "1.8",

	"background":{
	  "scripts": ["background.js"]
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"]
}